package semana_02;

public class Edificio {
	private int cod, numDpto,cantPiso;
	private double preDolar;
	
	public Edificio(int cod, int numDpto, int cantPiso, double preDolar) {
		this.cod = cod;
		this.numDpto = numDpto;
		this.cantPiso = cantPiso;
		this.preDolar = preDolar;
	}

	public int getCod() {
		return cod;
	}

	public void setCod(int cod) {
		this.cod = cod;
	}

	public int getNumDpto() {
		return numDpto;
	}

	public void setNumDpto(int numDpto) {
		this.numDpto = numDpto;
	}

	public int getCantPiso() {
		return cantPiso;
	}

	public void setCantPiso(int cantPiso) {
		this.cantPiso = cantPiso;
	}

	public double getPreDolar() {
		return preDolar;
	}

	public void setPreDolar(double preDolar) {
		this.preDolar = preDolar;
	}
	
	public double precioDolarEdif(){
		return numDpto * preDolar;
	}
	
	
}
